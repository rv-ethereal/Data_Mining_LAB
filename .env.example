# PostgreSQL Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=datalake_warehouse
POSTGRES_USER=datalake_user
POSTGRES_PASSWORD=datalake_password

# Data Lake Paths
DATALAKE_ROOT=c:/Users/tb619/Videos/Dm assignment/datalake
RAW_DATA_PATH=${DATALAKE_ROOT}/raw
STAGING_PATH=${DATALAKE_ROOT}/staging
PROCESSED_PATH=${DATALAKE_ROOT}/processed
WAREHOUSE_PATH=${DATALAKE_ROOT}/warehouse

# Spark Configuration
SPARK_MASTER=local[*]
SPARK_APP_NAME=DataLakeETL

# Airflow Configuration
AIRFLOW_HOME=c:/Users/tb619/Videos/Dm assignment/airflow
